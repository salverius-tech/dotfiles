{
  "editor.fontFamily": "JetBrainsMono Nerd Font, Consolas, 'Courier New', monospace",
  "editor.fontSize": 14,
  "editor.fontLigatures": true,
  "editor.lineHeight": 1.5,
  "editor.wordWrap": "wordWrapColumn",
  "editor.wordWrapColumn": 120,
  "editor.rulers": [80, 120],
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.trimAutoWhitespace": true,
  "editor.insertFinalNewline": true,
  "editor.trimTrailingWhitespace": true,
  "editor.autoIndent": "full",
  "editor.detectIndentation": true,
  "editor.tabSize": 2,
  "editor.smoothScrolling": true,
  "editor.cursorBlinking": "smooth",
  "editor.cursorStyle": "line",
  "editor.renderWhitespace": "boundary",
  "editor.renderLineHighlight": "line",
  "editor.minimap.enabled": true,
  "editor.minimap.scale": 1,
  "editor.minimap.renderCharacters": true,
  "editor.minimap.showSlider": "always",
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": true,
  "editor.guides.indentation": true,
  "editor.folding": true,
  "editor.foldingStrategy": "auto",
  "editor.showFoldingControls": "always",
  "editor.unfoldOnClickAfterEndOfLine": false,
  "editor.acceptSuggestionOnEnter": "smart",
  "editor.quickSuggestions": {
    "other": true,
    "comments": false,
    "strings": false
  },
  "editor.suggestSelection": "first",
  "editor.suggest.insertMode": "replace",
  "editor.suggest.showStatusBar": true,
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 1000,
  "files.trimTrailingWhitespace": true,
  "files.trimFinalNewlines": true,
  "files.insertFinalNewline": true,
  "files.defaultLanguage": "plaintext",
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/.vscode": false,
    "**/.chezmoi*": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/.hg/store/**": true
  },
  "workbench.colorTheme": "Default Dark+",
  "workbench.iconTheme": "vscode-icons",
  "workbench.tree.indent": 20,
  "workbench.editor.enablePreview": false,
  "workbench.editor.tabSizing": "shrink",
  "workbench.editor.wrapTabs": true,
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.value": 10,
  "workbench.startupEditor": "welcomePage",
  "workbench.commandPalette.experimental.enableNaturalLanguageSearch": true,
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font",
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.lineHeight": 1.2,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.scrollback": 10000,
  "terminal.integrated.enablePersistentSessions": true,
  "terminal.integrated.tabs.enabled": true,
  "terminal.integrated.profiles.windows": {
    "PowerShell": {
      "source": "PowerShell",
      "icon": "terminal-powershell"
    },
    "Command Prompt": {
      "path": [
        "${env:windir}\\Sysnative\\cmd.exe",
        "${env:windir}\\System32\\cmd.exe"
      ],
      "args": [],
      "icon": "terminal-cmd"
    },
    "Git Bash": {
      "source": "Git Bash"
    }
  },
  "git.enableSmartCommit": true,
  "git.confirmSync": false,
  "git.autofetch": true,
  "git.autofetchPeriod": 300,
  "git.showPushSuccessNotification": true,
  "git.openRepositoryInParentFolders": "always",
  "gitlens.currentLine.enabled": true,
  "gitlens.currentLine.pullRequests.enabled": true,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.statusBar.enabled": true,
  "gitlens.statusBar.alignment": "left",
  "gitlens.mode.statusBar.enabled": true,
  "gitlens.blame.toggleMode": "window",
  "gitlens.views.branches.avatars": false,
  "gitlens.views.commits.avatars": false,
  "gitlens.views.contributors.avatars": false,
  "gitlens.views.remotes.avatars": false,
  "gitlens.views.repositories.avatars": false,
  "gitlens.views.stashes.avatars": false,
  "gitlens.views.tags.avatars": false,
  "gitlens.views.worktrees.avatars": false,
  "docker.showStartPage": false,
  "docker.containers.groupBy": "ContainerName",
  "docker.containers.description": [
    "Status",
    "Image"
  ],
  "docker.containers.sortBy": "CreatedTime",
  "docker.containers.logViewer.timestampFormat": "MM/DD/YYYY HH:mm:ss",
  "docker.volumes.groupBy": "VolumeType",
  "docker.images.groupBy": "Repository",
  "docker.images.description": [
    "CreatedTime",
    "Size"
  ],
  "docker.images.showDanglingImages": false,
  "docker.networks.groupBy": "NetworkType",
  "docker.contexts.showInStatusBar": true,
  "docker.dockerPath": "docker",
  "docker.enableDockerComposeV2": true,
  "docker.explorer.showAzureTasks": false,
  "docker.explorer.refreshInterval": 3000,
  "prettier.enable": true,
  "prettier.printWidth": 120,
  "prettier.tabWidth": 2,
  "prettier.useTabs": false,
  "prettier.semi": true,
  "prettier.singleQuote": false,
  "prettier.trailingComma": "es5",
  "prettier.bracketSpacing": true,
  "prettier.arrowParens": "always",
  "prettier.endOfLine": "lf",
  "prettier.proseWrap": "preserve",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one",
    "editor.formatOnSave": true,
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 80
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml",
    "editor.formatOnSave": true
  },
  "[python]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "ms-python.python"
  },
  "[powershell]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "ms-vscode.powershell"
  },
  "remote.SSH.remotePlatform": {
    "*": "linux"
  },
  "remote.SSH.useLocalServer": true,
  "remote.SSH.connectTimeout": 60,
  "remote.WSL.fileWatcher.polling": true,
  "remote.WSL.server.autoStart": true,
  "remote.containers.defaultExtensions": [
    "eamodio.gitlens",
    "esbenp.prettier-vscode"
  ],
  "remote.containers.logLevel": "info",
  "extensions.autoUpdate": true,
  "extensions.autoCheckUpdates": true,
  "extensions.ignoreRecommendations": false,
  "security.workspace.trust.enabled": true,
  "security.workspace.trust.startupPrompt": "once",
  "security.workspace.trust.untrustedFiles": "prompt",
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/.git": true,
    "**/.github": false,
    "**/.chezmoi*": true
  },
  "search.useGlobalIgnoreFiles": true,
  "search.useIgnoreFiles": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmUndo": "default",
  "explorer.openEditors.sortOrder": "editorOrder",
  "explorer.openEditors.visible": 10,
  "breadcrumbs.enabled": true,
  "debug.toolBarLocation": "floating",
  "debug.internalConsoleOptions": "openOnSessionStart",
  "debug.console.acceptSuggestionOnEnter": "on",
  "debug.console.collapseIdenticalLines": true,
  "debug.showInlineValueWhenDebugging": true,
  "telemetry.telemetryLevel": "off",
  "workbench.enableExperiments": false,
  "update.mode": "start",
  "update.enableWindowsBackgroundUpdates": true,
  "settingsSync.keybindingsPerPlatform": true
}
